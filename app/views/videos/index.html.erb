<div class="row-video">
    <div class="col-3">
    </div>

    <div class="col-6">
		<h4 class="image_list">動画一覧</h4>
		    <% @videos.each do |video| %>
    		    <% if video.thumbnail.present? %>
        		    <%= link_to video_path(video.id) do %>
        		     <%= image_tag video.thumbnail.url, height: '240', width: '240' %>
        		    <% end %>
    		    <% end %>
		    <% end %>
    </div>
    <%= paginate @video_pages %>
    <div class="col-3">
    </div>
</div>